    "jsonwebtoken": "^9.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "sequelize-cli": "^6.6.2",
    "typescript": "~5.0.4",
    "vite": "^4.3.9",
    "vite-plugin-vue-devtools": "^1.0.0-rc.5",
