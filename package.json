    "egg-validate": "^2.0.2",
    "element-plus": "^2.9.8",
    "file-saver": "^2.0.5",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "mitt": "^3.0.1",
    "pinia": "^2.1.6",
    "uuid": "^11.1.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "xlsx": "^0.18.5",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/file-saver": "^2.0.7",
