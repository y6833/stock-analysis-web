{
  "name": "stock-analysis-web",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --skipLibCheck",
    "format": "prettier --write src/",
    "proxy": "node proxy-server.cjs",
