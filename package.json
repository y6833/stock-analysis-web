    "test:coverage": "vitest run src/tests --coverage",
    "test:ui": "vitest --ui src/tests",
    "test:server": "cd server && npm test"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "echarts": "^5.4.3",
    "egg-mysql": "^5.0.0",
